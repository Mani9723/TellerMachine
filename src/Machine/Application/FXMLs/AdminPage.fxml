<StackPane fx:id="stackpane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Machine.Application.Controllers.AdministratorController">
   <children>
      <ImageView fx:id="image" fitHeight="500.0" fitWidth="700.0" pickOnBounds="true">
         <image>
            <Image url="@../../images/3e59e879d4e8bd21233486167e4a2880.jpg" />
         </image>
      </ImageView>
      <AnchorPane fx:id="anchorPane" prefHeight="500.0" prefWidth="700.0">
         <children>
            <HBox layoutY="14.0" prefHeight="62.0" prefWidth="700.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <GridPane prefHeight="62.0" prefWidth="810.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <JFXButton fx:id="logoutButton" onAction="#handleHomeButton" prefHeight="32.0" prefWidth="173.0" styleClass="exitHome" stylesheets="@application.css" text="LOGOUT" textFill="black">
                           <GridPane.margin>
                              <Insets left="35.0" />
                           </GridPane.margin>
                           <font>
                              <Font name="Papyrus" size="12.0" />
                           </font>
                        </JFXButton>
                        <Label alignment="CENTER" prefHeight="17.0" prefWidth="203.0" text="ADMINISTRATOR" GridPane.columnIndex="1">
                           <font>
                              <Font name="Papyrus" size="16.0" />
                           </font>
                        </Label>
                        <Label fx:id="dateLabel" alignment="CENTER" prefHeight="17.0" prefWidth="233.0" text="12/29/2019" GridPane.columnIndex="2">
                           <font>
                              <Font name="Papyrus" size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </GridPane>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
